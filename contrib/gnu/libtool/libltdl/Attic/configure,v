head	1.3;
access;
symbols
	libtool-1_5_10:1.1.2.3
	libtool-1_5_8:1.1.2.2
	libtool-1_4_3:1.1.2.1
	FSF:1.1.2;
locks; strict;
comment	@# @;


1.3
date	2004.02.07.23.06.25;	author tg;	state dead;
branches;
next	1.2;

