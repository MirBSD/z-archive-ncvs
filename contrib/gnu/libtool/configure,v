ac_lib_var=$(echo dld'_'dld_link | sed 'y%./+-:%__p__%')
if eval "test \"\${ac_cv_lib_$ac_lib_var+set}\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  ac_save_LIBS="$LIBS"
LIBS="-ldld  $LIBS"
cat > conftest.$ac_ext <<EOF
#line 8633 "configure"
#include "confdefs.h"
