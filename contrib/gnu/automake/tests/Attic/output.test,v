# Test to make sure files listed in AC_OUTPUT are found.

. ./defs || exit 1

cat > configure.in << 'END'
AC_INIT
AM_INIT_AUTOMAKE(nonesuch, nonesuch)
AC_OUTPUT(Makefile frob/zardoz)
END

: > Makefile.am

mkdir frob
: > frob/zardoz.in

$ACLOCAL || exit 1
$AUTOMAKE
