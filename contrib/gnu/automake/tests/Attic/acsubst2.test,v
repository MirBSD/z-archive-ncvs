. ./defs || exit 1

set -e

cat >> configure.in << 'END'
AC_PROG_CC
AM_PROG_CC_C_O
AC_SUBST([FOOBAR_CFLAGS],[blablabla])
END

cat > Makefile.am << 'END'
bin_PROGRAMS = mumble
mumble_SOURCES = a.c b.c d.h
mumble_CFLAGS = $(FOOBAR_CFLAGS)
END

$ACLOCAL
$AUTOMAKE -a
