#ifndef _BASENAME_H
#define _BASENAME_H

/* This is where basename() is declared.  */
#include <string.h>


#ifdef __cplusplus
extern "C" {
#endif


#if !(__GLIBC__ >= 2)
/* When not using the GNU libc we use the basename implementation we
   provide here.  */
extern char *gnu_basename (const char *);
#define basename(Arg) gnu_basename (Arg)
#endif


#ifdef __cplusplus
}
#endif


#endif /* _BASENAME_H */
@


