XCOMM 	fs1 class
fs1	{
	charset	{
		name	GB2312.1980-0:GL
	}
	font	{
		primary	GB2312.1980-0:GL
		substitute	GB2312.1980-0:GLGR
	}
}
END XLC_FONTSET

