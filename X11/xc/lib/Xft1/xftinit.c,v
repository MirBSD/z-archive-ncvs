#include <stdlib.h>
#include "xftint.h"

XftFontSet  *_XftFontSet;
Bool	    _XftConfigInitialized;
static char *nodirs[] = { 0 };
char	    **XftConfigDirs = nodirs;

Bool
XftInit (char *config)
{
    if (_XftConfigInitialized)
	return True;
    if (!FcInit ())
	return False;
    _XftConfigInitialized = True;
    return True;
}
@


