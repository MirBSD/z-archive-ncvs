@/* Japanese messages are separate to another file.             */
/* Because Japanese EUC-JP encoding is conflict with ISO-8859. */
/* By: YOKOTA Hiroshi <yokota@@netlab.is.tsukuba.ac.jp>         */

#ifndef __XLOCK_MSG_JP_H__
#define __XLOCK_MSG_JP_H__

#define JP_TIME_ELAPSSED_MINUTES "ロックしてから %d 分が経過しました。                      \n"
#define JP_TIME_ELAPSSED_HOURS   "ロックしてから %d 時間 %02d 分経過しました。              \n"
#define JP_BUTTON_MINUTES        "%d 分後にログアウトボタンが出現します。                   \n"
#define JP_BUTTON_HOURS          "%d 時間 %02d 分後にログアウトボタンが出現します。         \n"
#define JP_AUTOLOGOUT_MINUTES    "自動ログアウトまで %d 分"
#define JP_AUTOLOGOUT_HOURS      "自動ログアウトまで %d 時間 %02d 分"

#endif
@


