head	1.5;
access;
symbols
	gcc-3_4-20050121:1.1.2.1
	gcc-3_4-20050114:1.1.2.1
	gcc-3_4-20041231:1.1.2.1
	FSF:1.1.2;
locks; strict;
comment	@# @;


1.5
date	2005.01.09.13.27.56;	author tg;	state Exp;
branches;
next	1.4;

