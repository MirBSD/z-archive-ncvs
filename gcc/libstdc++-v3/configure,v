cat >>conftest.$ac_ext <<_ACEOF
/* end confdefs.h.  */
#include <math.h>
int
main ()
{
 `for x in acosl asinl atanl \
                                          cosl sinl tanl \
                                          coshl sinhl tanhl; do echo "$x (0);"; done`
  ;
  return 0;
}
_ACEOF
rm -f conftest.$ac_objext
