_ACEOF

         cat >>confdefs.h <<\_ACEOF
#define HAVE_WCSRTOMBS 1
_ACEOF

         cat >>confdefs.h <<\_ACEOF
#define HAVE_WCSSPN 1
_ACEOF

         cat >>confdefs.h <<\_ACEOF
