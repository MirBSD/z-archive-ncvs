_ACEOF

else
  LIBMATHOBJS="$LIBMATHOBJS signbitf.lo"
fi
done


          if test x$ac_cv_func_copysignl = x"yes"; then

for ac_func in __signbitl
do
as_ac_var=`echo "ac_cv_func_$ac_func" | $as_tr_sh`
