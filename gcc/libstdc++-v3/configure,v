  if test x${glibcxx_cv_func_setenv_use+set} != xset; then
    if test "${glibcxx_cv_func_setenv_use+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else



      ac_ext=cc
ac_cpp='$CXXCPP $CPPFLAGS'
