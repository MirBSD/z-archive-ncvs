    cat >>confdefs.h <<\_ACEOF
#define HAVE_ISNANF 1
_ACEOF


    cat >>confdefs.h <<\_ACEOF
#define HAVE_MMAP 1
_ACEOF

    cat >>confdefs.h <<\_ACEOF
#define HAVE_ACOSF 1
_ACEOF

    cat >>confdefs.h <<\_ACEOF
