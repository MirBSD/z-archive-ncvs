#ifndef __layout_dump__
#define __layout_dump__


/*
 * Defines
 */


/*
 * Types
 */
enum {
    kEnd,
    kHex,
    kDec,
    kUns,
    kBit
};

typedef struct {
    short   byte_offset;
    short   bit_offset;
    short   bit_length;
    short   format;
    char    *name;
} layout;


/*
 * Global Constants
 */


/*
 * Global Variables
 */


/*
 * Forward declarations
 */


/*
 * Routines
 */
void dump_using_layout(void *buffer, layout *desc);
void DumpRawBuffer(unsigned char *bufferPtr, int length);

#endif /* __layout_dump__ */
