#include "config.h"

#ifndef lint
static const char sccsid[] = "@@(#)v_status.c	10.9 (Berkeley) 5/15/96";
#endif /* not lint */

#include <sys/types.h>
#include <sys/queue.h>
#include <sys/stat.h>
#include <sys/time.h>

#include <bitstring.h>
#include <limits.h>
#include <stdio.h>

#include "../common/common.h"
#include "vi.h"

/*
 * v_status -- ^G
 *	Show the file status.
 *
 * PUBLIC: int v_status(SCR *, VICMD *);
 */
int
v_status(sp, vp)
	SCR *sp;
	VICMD *vp;
{
	(void)msgq_status(sp, vp->m_start.lno, MSTAT_SHOWLAST);
	return (0);
}
