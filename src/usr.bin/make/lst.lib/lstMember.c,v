/*-
 * lstMember.c --
 *	See if a given datum is on a given list.
 */

#include    "lstInt.h"
#include    <stddef.h>

LstNode
Lst_Member(Lst l, void *d)
{
    LstNode		lNode;

    for (lNode = l->firstPtr; lNode != NULL; lNode = lNode->nextPtr)
	if (lNode->datum == d)
	    return lNode;
    return NULL;
}
