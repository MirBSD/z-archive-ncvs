#ifndef lint
#if 0
static char sccsid[] = "@@(#)wwredraw.c	8.1 (Berkeley) 6/6/93";
#else
static char rcsid[] = "$OpenBSD: wwredraw.c,v 1.5 2003/06/03 02:56:23 millert Exp $";
#endif
#endif /* not lint */

#include "ww.h"
#include "tt.h"

wwredraw()
{
	int i, j;
	union ww_char *os;

	xxclear();
	for (i = 0; i < wwnrow; i++) {
		wwtouched[i] = WWU_TOUCHED;
		os = wwos[i];
		for (j = wwncol; --j >= 0;)
			(os++)->c_w = ' ';
	}
}
