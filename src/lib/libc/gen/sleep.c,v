#if defined(LIBC_SCCS) && !defined(lint)
/* $OpenBSD: sleep.c,v 1.10 2004/05/18 02:05:52 jfb Exp $ */
#endif /* LIBC_SCCS and not lint */

#include <sys/cdefs.h>
#include <sys/time.h>
#include <unistd.h>

__RCSID("$MirBSD: src/lib/libc/gen/sleep.c,v 1.3 2004/06/23 08:24:30 tg Stab $");

unsigned int
sleep(unsigned int seconds)
{
	struct timespec rqt, rmt;

	if (seconds == 0)
		return(0);

	rqt.tv_sec = seconds;
	rqt.tv_nsec = 0;

	if (nanosleep(&rqt, &rmt) < 0)
		;

	return(rmt.tv_sec);
}
@


