#include <machine/asm.h>

	.intel_syntax noprefix
	.section .comment
	.asciz	"$MirBSD: src/lib/libc/arch/i386/stdlib/abs.S,v 1.5 2004/12/05 13:24:44 tg Exp $"
	.text

NENTRY(abs)
ENTRY(labs)
	mov	eax,[esp+4]
	or	eax,eax
	jns	pos
	neg	eax
pos:	ret
@


