/*
 * rpc protocol definition for (remote) wall protocol
 *
 * this has to be compatible with sun's definition
 */

#ifndef RPC_HDR
%#ifndef lint
%static char rcsid[] = "$OpenBSD: rwall.x,v 1.2 1997/08/19 07:54:51 niklas Exp $";
%#endif /* not lint */
#endif

program WALLPROG {
	version WALLVERS {
		void WALLPROC_WALL(string) = 2;
	} = 1;
} = 100008;
@


