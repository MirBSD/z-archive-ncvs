divert(0)dnl
VERSIONID(`$Sendmail: huginn.cs.mc,v 8.15 1999/02/07 07:26:03 gshapiro Exp $')
OSTYPE(hpux9)dnl
DOMAIN(CS.Berkeley.EDU)dnl
MASQUERADE_AS(CS.Berkeley.EDU)dnl
MAILER(local)dnl
MAILER(smtp)dnl

LOCAL_CONFIG
DDBerkeley.EDU

# hosts for which we accept and forward mail (must be in .Berkeley.EDU)
CF CS
FF/etc/sendmail.cw

LOCAL_RULE_0
R< @@ $=F . $D . > : $*		$@@ $>7 $2		@@here:... -> ...
R$* $=O $* < @@ $=F . $D . >	$@@ $>7 $1 $2 $3		...@@here -> ...

R$* < @@ $=F . $D . >		$#local $: $1		use UDB
@


