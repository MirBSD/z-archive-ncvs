#include <sm/gen.h>
SM_RCSID("@@(#)$Sendmail: clrerr.c,v 1.13 2001/09/11 04:04:48 gshapiro Exp $")
#include <sm/io.h>
#include <sm/assert.h>
#include "local.h"

/*
**  SM_IO_CLEARERR -- public function to clear a file pointer's error status
**
**	Parameters:
**		fp -- the file pointer
**
**	Returns:
**		nothing.
*/
#undef	sm_io_clearerr

void
sm_io_clearerr(fp)
	SM_FILE_T *fp;
{
	SM_REQUIRE_ISA(fp, SmFileMagic);

	sm_clearerr(fp);
}
@


