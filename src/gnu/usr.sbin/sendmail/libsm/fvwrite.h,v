/* I/O descriptors for sm_fvwrite() */
struct sm_iov
{
	void	*iov_base;
	size_t	iov_len;
};
struct sm_uio
{
	struct sm_iov	*uio_iov;
	int		uio_iovcnt;
	int		uio_resid;
};

extern int sm_fvwrite __P((SM_FILE_T *, int, struct sm_uio *));
