				|grep "($ac_cv_sizeof_long_double)" >/dev/null ; then
				ccvs_cv_unique_float_type_long_double=no
			else
				ccvs_cv_unique_float_type_long_double=yes\($ac_cv_sizeof_long_double\)
			fi
