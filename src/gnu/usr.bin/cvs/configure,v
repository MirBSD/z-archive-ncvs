  case $LIBOBJS in
    "lstat.$ac_objext"   | \
  *" lstat.$ac_objext"   | \
    "lstat.$ac_objext "* | \
  *" lstat.$ac_objext "* ) ;;
  *) LIBOBJS="$LIBOBJS lstat.$ac_objext" ;;
esac

fi








