/* end confdefs.h.  */
#include <stddef.h>
long longval () { return ~(size_t)0 / 10; }
unsigned long ulongval () { return ~(size_t)0 / 10; }
#include <stdio.h>
#include <stdlib.h>
int
main ()
{

  FILE *f = fopen ("conftest.val", "w");
  if (! f)
    exit (1);
  if ((~(size_t)0 / 10) < 0)
    {
      long i = longval ();
      if (i != (~(size_t)0 / 10))
	exit (1);
      fprintf (f, "%ld\n", i);
    }
  else
    {
      unsigned long i = ulongval ();
      if (i != (~(size_t)0 / 10))
	exit (1);
      fprintf (f, "%lu\n", i);
    }
  exit (ferror (f) || fclose (f) != 0);

  ;
  return 0;
}
