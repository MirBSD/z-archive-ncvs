  LOCAL_ZLIB_VERSION=`sed -n '/^#define ZLIB_VERSION ".*"$/{
                              s/^#define ZLIB_VERSION "\(.*\)"$/\1/;
                              p;}' <$srcdir/zlib/zlib.h 2>&5`
