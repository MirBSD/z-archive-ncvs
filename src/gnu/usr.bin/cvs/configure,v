#define SERVER_LO_WATER ($ccvs_lwm)
_ACEOF


cat >>confdefs.h <<_ACEOF
#define SERVER_HI_WATER ($ccvs_hwm)
_ACEOF

  fi # enable_server_flow_control
fi # enable_server




