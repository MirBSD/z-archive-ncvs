cat >>conftest.$ac_ext <<_ACEOF
/* end confdefs.h.  */
$ac_includes_default
int
main ()
{
struct stat s, t;
  exit (!(stat ("conftest.data", &s) == 0
	  && utime ("conftest.data", (long *)0) == 0
	  && stat ("conftest.data", &t) == 0
	  && t.st_mtime >= s.st_mtime
	  && t.st_mtime - s.st_mtime < 120));
  ;
  return 0;
}
