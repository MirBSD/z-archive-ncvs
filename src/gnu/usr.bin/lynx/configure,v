	test -n "$verbose" && echo "	... testing lib-directories under /opt"
	test -d /opt/lib &&             cf_search="$cf_search /opt/lib"
	test -d /opt/lib/bz2 &&          cf_search="$cf_search /opt/lib/bz2"
