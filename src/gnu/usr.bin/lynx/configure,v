  LIBS="-lmytinfo $LIBS"
else
  echo "$ac_t""no" 1>&6
fi

	;;
hpux10.*|hpux11.*) #(vi
	echo $ac_n "checking for initscr in -lcur_colr""... $ac_c" 1>&6
