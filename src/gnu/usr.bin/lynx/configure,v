echo "$ac_t""$gt_cv_func_gnugettext1_libc" 1>&6

	   if test "$gt_cv_func_gnugettext1_libc" != "yes"; then
	     echo $ac_n "checking for GNU gettext in libintl""... $ac_c" 1>&6
echo "configure:4437: checking for GNU gettext in libintl" >&5
if eval "test \"\${gt_cv_func_gnugettext1_libintl+set}\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  gt_save_LIBS="$LIBS"
		LIBS="$LIBS -lintl $LIBICONV"
		cat > conftest.$ac_ext <<EOF
#line 4444 "configure"
#include "confdefs.h"
#include <libintl.h>
extern int _nl_msg_cat_cntr;
int main() {
bindtextdomain ("", "");
return (int) gettext ("") + _nl_msg_cat_cntr
; return 0; }
EOF
if { (eval echo configure:4453: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
  rm -rf conftest*
