echo "configure:7283: checking if we have identified curses headers" >&5
if eval "test \"\${cf_cv_ncurses_header+set}\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  
cf_cv_ncurses_header=none
for cf_header in \
	curses.h \
	ncurses.h \
	ncurses/curses.h \
	ncurses/ncurses.h
do
