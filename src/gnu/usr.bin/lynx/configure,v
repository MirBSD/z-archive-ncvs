#include <netinet/in.h>
#ifdef __KAME__
yes
#endif
EOF
if (eval "$ac_cpp conftest.$ac_ext") 2>&5 |
  egrep "yes" >/dev/null 2>&1; then
  rm -rf conftest*
  cf_cv_ipv6type=$i
fi
rm -f conftest*

		;;
	linux-glibc) #(vi
				cat > conftest.$ac_ext <<EOF
