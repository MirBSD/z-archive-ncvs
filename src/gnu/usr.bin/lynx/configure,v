if test ".$use_bzlib" != ".no" ; then
	
echo $ac_n "checking if we need to add -I option to get bzlib.h""... $ac_c" 1>&6
echo "configure:15316: checking if we need to add -I option to get bzlib.h" >&5
cat > conftest.$ac_ext <<EOF
