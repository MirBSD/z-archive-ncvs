#else
#ifdef HAVE_PATHS_H
#include <paths.h>
#endif
#endif
int main() {
char *path = _PATH_LASTLOG
; return 0; }
EOF
if { (eval echo configure:13311: \"$ac_compile\") 1>&5; (eval $ac_compile) 2>&5; }; then
  rm -rf conftest*
  cf_cv_path_lastlog="_PATH_LASTLOG"
else
