#define RC4STATE 256
#define RC4KEYLEN 16

struct rc4_ctx {
	u_int8_t x, y;
	u_int8_t state[RC4STATE];
};

void	rc4_keysetup(struct rc4_ctx *, u_char *, u_int32_t);
void	rc4_crypt(struct rc4_ctx *, u_char *, u_char *, u_int32_t);
@


