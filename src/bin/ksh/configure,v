#line 3510 "configure"
#include "confdefs.h"

#include <stdio.h>
#include <sys/types.h>
#include <sys/stat.h>
	  main()
	  {
	    struct stat s1, s2;
	    FILE *fp1, *fp2;
	    char *file = "conftest.file";
	    char devfd[32];

	    if (!(fp1 = fopen(file, "w")))
	      exit(1);
	    if (fstat(fileno(fp1), &s1) < 0)
	      exit(2);
	    sprintf(devfd, "/dev/fd/%d", fileno(fp1));
	    if (!(fp2 = fopen(devfd, "w")))
	      exit(3);
	    if (fstat(fileno(fp2), &s2) < 0)
	      exit(4);
	    if (s1.st_dev != s2.st_dev || s1.st_ino != s2.st_ino)
	      exit(5);
	    exit(0);
	  }
	
EOF
if { (eval echo configure:3538: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext} && (./conftest; exit) 2>/dev/null
